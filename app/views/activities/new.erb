<h1>activity new form </h1>

<%= form_for @activity do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :season %>
    <%= f.text_field :season %>

    <%= f.label :location %>
    <%= f.text_field :location %>

    <%= f.label :activity_time %>
    <%= f.datetime_field :activity_time %>


    <%= f.submit %>

<% end %>